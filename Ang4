+++++++++++++++++++++++++++++++++++++++++

var = mutable (allows redefining, if of the same data type )
    eg.1. Allowed
    var x:string = 'x';
    var x:string = 'y'; //redefining is allowed
   
    eg. 2. NOT Allowed
    var x:string = 'x';
    var x:number = 10; //error
   
let = mutable (does not allow redefining)
    let x:string = 'x';
    let x:string = 'y'; //redefining is NOT allowed
   
const = immutable

