     const x1 = 10;
     const x2 = { name: 'Sachin', lastname: 'Tendulkar'};
     return {
         xone: x1,
         xtwo: x2
     };
 }
 let someobj = somefunc();
 console.log(someobj.xone, someobj.xtwo);


 Now:-

function somefunc(){
     const x1 = 10;
     const x2 = { name: 'Sachin', lastname: 'Tendulkar'};
     const x3 = new Array(3, 10, 20, 30); //3 elements and the elements are 10, 20, 30.
     return {
        x1, x2, x3
     };
 }

 //---

