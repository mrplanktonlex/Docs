eg.3.

function  t1() {
    console.log('t1 is done');
} //t1 ends


function  t2() {
    setTimeout(()=>{console.log('t2 is done');}, 3000);
} //t2 ends

function  t3() {
    console.log('t3 is done');
} //t3 ends

Promise.race([t3(), t2(), t1() ]);  //sequence is not maintained. t3, t1, and t2.
    //t2 is async.
   
